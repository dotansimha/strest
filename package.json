{
  "name": "strest",
  "version": "0.0.36",
  "description": "Stress tests",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "predev": "npm run build",
    "dev": "cd dist && node strest.js ../test-example/*.strest.ts"
  },
  "bin": {
    "strest": "dist/strest.js"
  },
  "keywords": [],
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": false,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/src/transformer/typescript.js"
    },
    "testRegex": ".*?",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/chalk": "^0.4.31",
    "@types/expect": "^1.20.0",
    "@types/jest": "^20.0.1",
    "assert": "^1.4.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "ddp": "^0.12.1",
    "expect": "^1.20.2",
    "glob": "^7.1.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "meteor-typings": "^1.3.1",
    "node-fetch": "^1.7.1",
    "reflect-metadata": "^0.1.9",
    "request": "^2.79.0",
    "rxjs": "^5.0.3",
    "typescript": "^2.1.4"
  },
  "devDependencies": {
    "@types/commander": "^2.3.31",
    "@types/glob": "^5.0.30",
    "@types/mkdirp": "^0.3.29",
    "@types/node": "^6.0.54",
    "@types/request": "^0.0.37",
    "conventional-changelog": "^1.1.0",
    "cz-conventional-changelog": "^1.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
